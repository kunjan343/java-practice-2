      <AnchorPane prefHeight="365.0" prefWidth="640.0">
         <children>
            <TextField fx:id="txtLastName" layoutX="414.0" layoutY="142.0" />
            <Text layoutX="52.0" layoutY="73.0" strokeType="OUTSIDE" strokeWidth="0.0" text="My Really Cool Contact Management System">
               <font>
                  <Font size="27.0" />
               </font>
            </Text>
            <TextField fx:id="txtFirstName" layoutX="199.0" layoutY="142.0" />
            <Text layoutX="199.0" layoutY="127.0" strokeType="OUTSIDE" strokeWidth="0.0" text="First Name" />
            <Text layoutX="414.0" layoutY="127.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Last Name" />
            <Text layoutX="414.0" layoutY="219.0" strokeType="OUTSIDE" strokeWidth="0.0" text="E-mail" />
            <Text layoutX="199.0" layoutY="219.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Phone" />
            <TextField fx:id="txtPhone" layoutX="199.0" layoutY="234.0" />
