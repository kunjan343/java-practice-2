      <Button fx:id="btnOk" layoutX="109.0" layoutY="289.0" mnemonicParsing="false" text="OK">
         <tooltip>
            <Tooltip text="Tooltip from Scene Builder" />
         </tooltip></Button>
      <Button fx:id="btnCancel" layoutX="228.0" layoutY="289.0" mnemonicParsing="false" text="Clear" />
      <Label layoutX="139.0" layoutY="42.0" text="User Form">
         <font>
            <Font size="27.0" />
         </font>
      </Label>
      <TextField fx:id="txtFirstName" layoutX="107.0" layoutY="109.0" promptText="First Name" />
      <TextField fx:id="txtLastName" layoutX="107.0" layoutY="176.0" promptText="Last Name" />
   </children>
</AnchorPane>
